n  const textShapes = font?.generateShapes(buffer, fontSize);\n  const cursorShapes = font?.generateShapes(\n    buffer.slice(0, cursorIndex),\n    fontSize\n  );\n  const cursorPosition = getCursorPosition({\n    position,\n    width,\n  });\n\n  return (\n    <group position={position.raw()}>\n      <Text\n        myRef={cursorTextRef}\n        position={cursorPosition}\n        padding={padding}\n        shapes={cursorShapes}\n        isTransparent\n      />\n      <Text\n        myRef={textRef}\n        position={cursorPosition}\n        padding={padding}\n        shapes={textShapes}\n        color={color}\n      />\n      <TextCursor\n        textMesh={cursorTextRef.current}\n        position={cursorPosition}\n        fontSize={fontSize}\n      />\n    </group>\n  );\n}\n"},"lineCount":null}},"error":null,"hash":"0f98a9525a226c918084626021fa1791","cacheData":{"env":{}}}